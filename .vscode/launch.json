{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/app/main.go",
			"buildFlags": "-tags hints --ldflags='-X github.com/stnokott/r6-dissect-influx/internal/constants.Version=0.0.1 -X github.com/stnokott/r6-dissect-influx/internal/constants.Commit=123456a -X github.com/stnokott/r6-dissect-influx/internal/constants.ProjectName=r6-dissect-influx'",
		},
		{
			"name": "Wails: Production",
			"type": "go",
			"request": "launch",
			"mode": "exec",
			"program": "${workspaceFolder}/build/bin/r6-dissect-influx-wails.exe",
			"preLaunchTask": "build",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Wails: Debug",
			"type": "go",
			"request": "launch",
			"mode": "exec",
			"program": "${workspaceFolder}/build/bin/r6-dissect-influx-wails.exe",
			"preLaunchTask": "build debug",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Wails: Dev",
			"type": "go",
			"request": "launch",
			"mode": "exec",
			"program": "${workspaceFolder}/build/bin/r6-dissect-influx-wails.exe",
			"preLaunchTask": "build dev",
			"cwd": "${workspaceFolder}"
		}
	]
}
